#include<stdio.h>

int main(void)
{
    int a=10;
    int *ptr=&a;
    printf("before change\n stored in ptr: %p\n", ptr);
    printf("value retured from ptr: %d\n", *ptr);

    ptr=ptr+3;
    printf("after change\n stored in ptr: %p\n", ptr);
    printf("value retured from ptr: %d\n", *ptr);

    return 0;
}
