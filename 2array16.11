#include<stdio.h>

int findSum(int arr[], int n);

int main(void)
{
    int n;

    printf("Enter value of n:");
    scanf("%d", &n);

    int arr[n];
    for(int i=0; i<n; i++)
    {
        printf("Enter value of arr[%d]:", i);
        scanf("%d", &arr[i]);
    }

    int sum = findSum(arr, n);
    printf("Sum: %d\n", sum);


    return 0;
}

int findSum(int arr[], int n)
{
    int sum = 0;
    for(int i=0; i<n; i++)
    {
        sum = sum + arr[i];
    }
    return sum;
}
