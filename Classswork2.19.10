#include<stdio.h>

int main(void)

{
    int num;

    //Getting input
    printf("Enter a number:");
    scanf("%d",&num);

    //Using if-else to check the condition
    if(num %5==0|| num %11==0)
    {
        printf("\nThe number is divisible by both 5 and 11\n");
    }
    else
    {
        printf("\nThe number is not divisible by both 5 and 11\n");
    }

    return 0;
}
