#include<stdio.h>

int findMin(int, int, int,int);

int main(void)
{
    int num1, num2, num3, num4;

    printf("Enter value of the 1st number:");
    scanf("%d", &num1);

    printf("Enter value of the 2nd number:");
    scanf("%d", &num2);

    printf("Enter value of the 3rd number:");
    scanf("%d", &num3);

    printf("Enter value of the 4th number:");
    scanf("%d", &num4);

    findMin(num1, num2, num3, num4);

    return 0;
}
int findMin(int num1, int num2, int num3,int num4)
{
    if(num1 < num2 && num1 < num3 && num1 <
       num4)
    {
        printf("The minimum value is %d", num1);
    }
    else if(num2 < num3 && num2 < num4 && num2 < num1)
    {
        printf("The minimum value is %d", num2);
    }
    else if(num3 < num4 && num3 < num1 && num3 < num2)
    {
        printf("The minimum value is %d", num3);
    }
    else if(num4 < num3 && num4 < num1 && num4 < num2)
    {
        printf("The minimum value is %d", num4);
    }
    else if(num4 == num3 && num4 == num1 && num4 == num2)
    {
        printf("The minimum value is %d", num4);
    }
    else
    {
        printf("Invalid input.");
    }
}
