#include<stdio.h>

int main(void)
{
    int n;
    printf("Enter value of n:");
    scanf("%d", &n);

    for(int i=1; i<=n; i++) //outer loop defines number of row
    {
        for(int j=1; j<=i; j++) ////inner loop defines number of coloumn
        {
            printf("* ");
        }
        printf("\n");
    }

    return 0;

}
