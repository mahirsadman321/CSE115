#include<stdio.h>

int nestedFor(int, int, int);

int main(void)
{
    int n;
    int i, j;
    printf("Enter value of n:");
    scanf("%d", &n);

    nestedFor(i, j, n);


    return 0;
}

int nestedFor(int i, int j, int n)
{
    for(int i=1; i<=n; i++) //outer loop defines number of row
    {
        for(int j=1; j<=n; j++) ////inner loop defines number of coloumn
        {
            printf("* ");
        }
        printf("\n");
    }

}
